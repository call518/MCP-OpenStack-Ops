# MCP Server Configuration
# Copy this file to .env and modify values as needed

# Logging level
MCP_LOG_LEVEL=INFO

# Python module path for Docker
PYTHONPATH=/app/src

# Transport configuration
FASTMCP_TYPE=stdio
# FASTMCP_TYPE=streamable-http

# HTTP Transport settings (used only when FASTMCP_TYPE=streamable-http)
FASTMCP_HOST=0.0.0.0
FASTMCP_PORT=8000

# Additional settings (if needed)
# DB_HOST=localhost
# DB_PORT=5432
# API_KEY=your-api-key-here

# OpenStack API environment variables
OS_PROJECT_DOMAIN_NAME=Default
OS_USER_DOMAIN_NAME=Default
OS_PROJECT_NAME=admin
OS_TENANT_NAME=admin
OS_USERNAME=admin
OS_PASSWORD=changeme
OS_AUTH_URL=http://192.168.35.2:5555
OS_INTERFACE=internal
OS_ENDPOINT_TYPE=internalURL
OS_IDENTITY_API_VERSION=3
OS_REGION_NAME=RegionOne
OS_AUTH_PLUGIN=password

# Docker Port Mappings
DOCKER_EXTERNAL_PORT_OPENWEBUI=3005
DOCKER_EXTERNAL_PORT_MCP_SERVER=18005
DOCKER_EXTERNAL_PORT_MCPO_PROXY=8005